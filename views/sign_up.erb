<html>
  <head>
    <title>Restaurant POS</title>
    <link rel="stylesheet" href="/stylesheets/style_sign_up.css">
    <link href='http://fonts.googleapis.com/css?family=Orbitron:700' rel='stylesheet' type='text/css'>
  </head>
  <body>
      <h1>Restaurant Name</h1>

    <section>
      <h2>POS SYSTEM</h2>
      <h3>Sing Up</h2>
      <form class="log-in" action="/users" method="post">
        <input class="log-form" type="text" name="user[username]" placeholder="...username"><br>
        <input class="log-form" type="password" name="password" placeholder="...password"><br>
        <input class="log-button" type="submit">
      </form>
    </section>

    <script charset="utf-8" src='/javascripts/lib/jquery.js'></script>
    <script charset="utf-8" src='/javascripts/lib/underscore.js'></script>
    <script charset="utf-8" src='/javascripts/lib/backbone.js'></script>
    <script charset="utf-8" src='/javascripts/app.js'></script>
  </body>
</html>
